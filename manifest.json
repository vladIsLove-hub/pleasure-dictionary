{
    "name": "Pleasure dictionary",
    "version": "1.0.0",
    "description": "One of the best extension of 'PLEASURE Technology Stack' helps you save your words for learn",
    "manifest_version": 3,
    "icons": {
        "16": "logo.png"
    },
    "author": "Vladislav Evtushenko",
    "permissions": [
        "storage",
        "downloads"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "index.js"
            ]
        }
    ],
    "action": {
        "default_popup": "index.html",
        "default_title": "Pleasure Dictionary"
    }
}